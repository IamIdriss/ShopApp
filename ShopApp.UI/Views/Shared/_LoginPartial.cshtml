<ul class="navbar-nav">
    @if (User.Identity.IsAuthenticated)
    {
        <li class="nav-item ">
            <a class="nav-link text-dark font22" href="#">Welcome @User.Identity.Name</a>
        </li>
        <li class="nav-item ">
            <a class="nav-link  text-dark font22" asp-action="Logout" asp-controller="Home">Logout</a>
        </li>
    }
    else
    {
        <li class="nav-item ">
            <a class="nav-link  text-dark font22" asp-action="Login" asp-controller="Home">Login</a>
        </li>
        <li class="nav-item ">
            <a class="nav-link  text-dark font22" asp-action="Register" asp-controller="Home">Register</a>
        </li>
    }
</ul>
